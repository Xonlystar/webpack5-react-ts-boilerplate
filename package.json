{
  "name": "webpack5-react-ts-boilerplate",
  "version": "1.0.0",
  "description": "从0开始搭建基于webpack5搭建的react + ts + antd模版",
  "author": "onlyxhb@qq.coom",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xonlystar/webpack5-react-ts-boilerplate.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Xonlystar/webpack5-react-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/Xonlystar/webpack5-react-ts-boilerplate#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-proposal-optional-chaining": "7.11.0",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/classnames": "^2.2.10",
    "@types/loadable__component": "^5.13.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.4",
    "@typescript-eslint/parser": "^4.1.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "mini-css-extract-plugin": "^1.1.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^4.0.4",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "react-css-modules": "^4.7.11",
    "react-dev-utils": "^10.2.1",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recess-order": "^2.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "~4.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.3",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0",
    "webpackbar": "^4.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "commitlint": "^8.2.0"
  },
  "dependencies": {
    "antd": "^4.6.4",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "mobx": "^5.15.6",
    "mobx-react": "^6.3.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.2.0"
  }
}
